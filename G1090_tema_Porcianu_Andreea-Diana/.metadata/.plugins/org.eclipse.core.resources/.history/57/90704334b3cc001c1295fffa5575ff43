package observer_3.clase;

public class Calator implements InterfataCalator{
	
	
	private String numeCalatorString;
	
	

	public Calator(String numeCalatorString) {
		super();
		this.numeCalatorString = numeCalatorString;
	}



	@Override
	public void primireAnunt(String anunt) {
		System.out.println("Dl/Dna "numeCalatorString+" ati primit "+anunt+".");
		
	}

}
